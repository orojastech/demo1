{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "verbose": true,
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "./src/**"
  ],
  "coverageDirectory": "./reports/coverage",
  "testResultsProcessor": "jest-sonar-reporter"
}
